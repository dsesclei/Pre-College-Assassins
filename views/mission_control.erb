                    <div class="field">
                        <label for="id_room">Room</label><span class="targetInfo"><%= @target.room %></span>
                    </div><!--END #field-->

                    <div class="field">
                        <label for="id_kills">Kills</label><span class="targetInfo"><%= @target.kills %></span>
                         <p class="help_text" style="float:right">Number of Kills your Target has had. The higher the number the better assassin he/she is. Watch out!</p>
                    </div><!--END #field-->
                    <div class="field">
                        <label for="id_passcode">Passcode</label>
                        <input name="target_code" maxlength="75" autocapitalize="off" autocorrect="off" type="text" class="required" id="target_code">
                         <p class="help_text" style="float:right">Your targets passcode. They should have given it to you when you killed them.</p>
                    </div><!--END #field-->
                    
                    <div class="field">
                      <input class="red_button" type="submit" value="Log Kill">
                    </div><!--END #field-->
				</form>
